 <h1>my page</h1>
<ul>
  <% @tasks.each do |task|%>
    <li>
    <%= link_to  task.title, task_path(task) %>
    <%= link_to task_path(task), method: :delete, data: { confirm: "Delete #{task.title}?" } do %>
        <i class="fa fa-trash-o" aria-hidden="true"></i>
      <% end %>
    </li>

  <% end %>
    <p>
    <%= link_to "Add a new task",tasks_new_path(@task) %>
    </p>
</ul>

